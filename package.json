{
  "name": "ChabapApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "env:loc": "node scripts/set-environment.js local",
    "env:dev": "node scripts/set-environment.js development",
    "env:prod": "node scripts/set-environment.js production",
    "_ios": "react-native run-ios",
    "_android": "react-native run-android",
    "ios:loc": "yarn env:loc && yarn _ios",
    "ios:dev": "yarn env:dev && yarn _ios",
    "ios:prod": "yarn env:prod && yarn _ios",
    "android:loc": "yarn env:loc && yarn _android && yarn android:adb",
    "android:dev": "yarn env:dev && yarn _android",
    "android:prod": "yarn env:prod && yarn _android",
    "_build:ios": "react-native bundle --dev false --entry-file index.jsx --bundle-output ios/main.jsbundle --assets-dest ./ios --platform ios",
    "build:ios:loc": "yarn env:loc && yarn _build:ios",
    "build:ios:dev": "yarn env:dev && yarn _build:ios",
    "build:ios:prod": "yarn env:prod && yarn _build:ios",
    "_build:android": "react-native bundle --platform android --dev false --entry-file index.jsx --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/react",
    "build:android:loc": "yarn env:loc && yarn _build:android",
    "build:android:dev": "yarn env:dev && yarn _build:android",
    "build:android:prod": "yarn env:prod && yarn _build:android",
    "android:adb": "adb -s emulator-5554 reverse tcp:8080 tcp:8080",
    "test": "jest",
    "lint": "eslint --ext .jsx --ext .js src/",
    "lint:fix": "eslint --ext .jsx --ext .js src/ --fix",
    "fast:build": "cd ios && fastlane firebase type:build",
    "fast:version": "cd ios && fastlane firebase type:version",
    "firebase:dev:build": "yarn build:ios:dev && yarn fast:build",
    "firebase:dev:version": "yarn build:ios:dev && yarn fast:version"
  },
  "dependencies": {
    "@bam.tech/react-native-make": "^3.0.3",
    "@react-native-community/hooks": "^2.8.1",
    "@react-native-firebase/analytics": "^14.2.1",
    "@react-native-firebase/app": "^14.2.1",
    "@react-native-firebase/messaging": "^14.2.1",
    "@react-native-picker/picker": "^2.2.1",
    "@react-navigation/bottom-tabs": "^6.0.9",
    "@react-navigation/drawer": "^6.1.8",
    "@react-navigation/material-top-tabs": "^6.0.6",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.5",
    "jetifier": "^2.0.0",
    "lottie-react-native": "^5.0.1",
    "react": "17.0.2",
    "react-native": "0.66.2",
    "react-native-device-info": "^8.4.8",
    "react-native-geolocation-service": "^5.3.0-beta.4",
    "react-native-gesture-handler": "^2.1.0",
    "react-native-image-crop-picker": "^0.37.2",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-nmap": "^0.0.66",
    "react-native-pager-view": "^5.4.9",
    "react-native-permissions": "^3.2.0",
    "react-native-reanimated": "^2.2.4",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.9.0",
    "react-native-send-intent": "^1.3.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-tab-view": "^3.1.1",
    "react-native-vector-icons": "^9.1.0",
    "react-native-version-info": "^1.1.1",
    "react-native-webview": "^11.18.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/eslint-config": "^2.0.0",
    "axios": "^0.24.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "7.24.0",
    "husky": "4",
    "jest": "^26.6.3",
    "jwt-decode": "^3.1.2",
    "lint-staged": "^11.0.0",
    "metro-react-native-babel-preset": "^0.66.2",
    "moment": "^2.29.1",
    "prettier": "^2.4.1",
    "react-native-paper": "^4.10.0",
    "react-native-popup-menu": "^0.15.12",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-status-bar-height": "^2.6.0",
    "react-native-svg": "^12.1.1",
    "react-native-svg-transformer": "^1.0.0",
    "react-test-renderer": "17.0.2",
    "styled-components": "^5.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.@(js|jsx)": [
      "yarn lint:fix",
      "git add"
    ]
  },
  "jest": {
    "preset": "react-native"
  }
}
